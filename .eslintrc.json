{
    "extends": ["google", "eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "semi": 0,
        "comma-dangle": 0,
        "object-curly-spacing": 0,
        "require-jsdoc": 0
   }
}
